using System;
					
public class Program
{
	public bool countBoolean(bool x , bool y,bool z)
	{
		if(x=y=z==true)
		{
		return true;
		}
		if(x==true && y==true || y==true && z==true || x==true && z==true)
		{
			return true;
		}
		else
		{
			return false;
		}
	}
	public static void Main()
	{
		Program p=new Program();
		Console.WriteLine(p.countBoolean(true,false,true));
	}
}

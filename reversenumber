using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;
using System.Threading.Tasks;

namespace luckynumber
{
    class ReverseNumber
    {
        static void Main(string[] args)
        {
            int rev =0;
            int num = 15442;
            while(num>0)
            {
                int digit = num % 10;
                rev = rev * 10 + digit;
                num /= 10;
            }
            Console.WriteLine($"rev number is:{rev}");
        }
    }
}
